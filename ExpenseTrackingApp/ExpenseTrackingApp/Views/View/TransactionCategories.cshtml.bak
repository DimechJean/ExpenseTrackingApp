@model ExpenseTrackingApp.Models.TransactionCategory

@{
    ViewBag.Title = "TransactionCategories";
    Layout = "~/Views/Shared/_Layout.cshtml";
    List<ExpenseTrackingApp.Models.TransactionCategory> categories = ViewData["CatList"] as List<ExpenseTrackingApp.Models.TransactionCategory>;
}

<h2>Transaction Categories</h2>

<div>
    <hr />
	<ul class="list-group">
		@foreach (var category in categories)
		{
			<li class="list-group-item"> @category.NameCat </li>
		}
	</ul>
	<a href="@Url.Action("Add", "AddCategory")" class="btn btn-primary btn-lg">Add Category &raquo;</a>

</div>

